{
    "name": "aiida_compressedsinglefile",
    "author": "Kevin M. Jablonka",
    "author_email": "kevin.jablonka@epfl.ch",
    "description": "AiiDA plugin that provides CompressedSinglefileData",
    "url": "https://github.com/kjappelbaum/aiida-compressedsinglefiedata",
    "license": "MIT",
    "classifiers": [
        "Programming Language :: Python",
        "Framework :: AiiDA"
    ],
    "version": "0.0.0a1",
    "entry_points": {
        "aiida.data": [
            "aiida.compressedsinglefile = aiida_compressedsinglefile:CompressedSinglefileData"]
    },
    "setup_requires": [
        "reentry"
    ],
    "reentry_register": true,
    "install_requires": [
        "aiida-core >=1.0.0b3,<2.0.0",
        "six"
    ],
    "extras_require": {
        "testing": [
            "pgtest==1.2.0",
            "pytest>=4.4,<5.0.0",
            "pytest-cov>=2.6.1,<3.0.0",
            "coverage"
        ],
        "pre-commit": [
            "pre-commit==1.11.0",
            "yapf==0.26.0",
            "prospector==0.12.11",
            "pylint==1.9.4"
        ]
    }
}
